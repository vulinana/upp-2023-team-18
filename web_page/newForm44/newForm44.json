        "\t$data.zahtevZaKodifikaciju.stavke = $data.zahtevZaKodifikaciju_stavke;",
        "\t$data.zahtevZaKodifikaciju.status = $data.zahtevZaKodifikaciju_status;",
        "\t$data.zahtevZaKodifikaciju.ugovor = $data.zahtevZaKodifikaciju_ugovor;",
        "\treturn {",
        "\t\t//map zahtevZaKodifikaciju variable to expected task contract input",
        "\t\tzahtevZaKodifikacijuInput: {",
        "\t\t\tstavke: $data.zahtevZaKodifikaciju.stavke.map( it => ({",
        "\t\t\t\tpersistenceId_string: it.persistenceId_string !== undefined ? it.persistenceId_string : null,",
        "\t\t\t\tdokumentZakona: it.dokumentZakona !== undefined ? it.dokumentZakona : null,",
        "\t\t\t\tinstrukcijeZaKodifikaciju: it.instrukcijeZaKodifikaciju !== undefined ? it.instrukcijeZaKodifikaciju : null,",
